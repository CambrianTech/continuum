{
  "name": "browser-command-daemon",
  "version": "1.0.0",
  "description": "Browser command execution daemon for Continuum",
  "main": "CommandDaemon.ts",
  "type": "module",
  "continuum": {
    "type": "browser-daemon",
    "priority": 30,
    "core": "command",
    "category": "Browser",
    "capabilities": [
      "command-execution",
      "console-forwarding",
      "command-discovery"
    ],
    "dependencies": [],
    "interfaces": ["WebSocket", "Console"],
    "permissions": ["execute-commands", "forward-console"],
    "quality": {
      "status": "active",
      "eslint": {
        "enforce": true,
        "level": "warn"
      },
      "typescript": {
        "noAny": true,
        "strict": true
      },
      "tests": {
        "required": true,
        "coverage": 80,
        "types": ["unit", "integration"]
      }
    }
  },
  "keywords": [
    "command",
    "daemon",
    "browser",
    "continuum"
  ],
  "author": "Continuum AI Platform",
  "license": "MIT",
  "scripts": {
    "test": "npm exec tsx test/",
    "test:unit": "npm exec tsx test/unit/",
    "test:integration": "npm exec tsx test/integration/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}