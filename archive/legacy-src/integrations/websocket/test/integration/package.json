{
  "name": "@continuum/websocket-integration-tests",
  "version": "1.0.0",
  "description": "End-to-end integration tests for WebSocket daemon command execution flow",
  "type": "module",
  "scripts": {
    "test": "jest --testMatch='**/*.integration.test.ts'",
    "test:watch": "jest --testMatch='**/*.integration.test.ts' --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --testMatch='**/*.integration.test.ts' --runInBand"
  },
  "dependencies": {
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/*.integration.test.ts"],
    "testTimeout": 10000
  }
}