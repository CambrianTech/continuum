{
  "active_example": "test-bench",
  "port_assignment": {
    "base_port": 9000,
    "auto_assign": true,
    "reserved_ports": [3000, 8080, 8000]
  },
  "examples": {
    "test-bench": {
      "name": "JTAG Test Bench", 
      "description": "Full-featured testing environment with comprehensive UI",
      "paths": {
        "directory": "examples/test-bench",
        "html_file": "public/demo.html",
        "build_output": "dist"
      },
      "services": ["http_server", "websocket_server"],
      "features": {
        "browser_automation": true,
        "screenshot_testing": true,
        "chat_integration": true,
        "widget_testing": false,
        "auto_launch_browser": true
      }
    },
    "widget-ui": {
      "name": "JTAG Widget Development UI",
      "description": "Focused widget development environment", 
      "paths": {
        "directory": "examples/widget-ui",
        "html_file": "index.html",
        "build_output": "dist"
      },
      "services": ["http_server", "websocket_server"],
      "features": {
        "browser_automation": false,
        "screenshot_testing": false,
        "chat_integration": true,
        "widget_testing": true,
        "auto_launch_browser": true
      }
    }
  }
}