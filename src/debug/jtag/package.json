    "build:ts": "tsc --project tsconfig.json",
    "build:mcp": "npx tsc mcp/jtag-mcp-server.ts --outDir mcp --target ES2020 --module commonjs --esModuleInterop",
    "version:bump": "npm version patch --no-git-tag-version",
    "system:start": "tmux new-session -d -s jtag-test 'npm start' || echo 'System already running'",
    "system:stop": "tmux kill-session -t jtag-test 2>/dev/null || true; lsof -ti:9002 | xargs kill -9 2>/dev/null || true; lsof -ti:9001 | xargs kill -9 2>/dev/null || true",
    "system:wait": "sleep 8 && curl -s http://localhost:9002 >/dev/null && curl -s http://localhost:9001 >/dev/null",
    "logs:show": "echo 'ðŸ“‹ JTAG Log Status:' && ls -la ../../../.continuum/jtag/logs/ 2>/dev/null || echo 'No logs found'",
    "logs:tail": "find ../../../.continuum/jtag/logs -name '*.log' -type f | head -1 | xargs tail -f 2>/dev/null || echo 'No log files to tail'",
    "test": "npm run test:all",
    "test:all": "npm run test:layer-1 && npm run test:layer-2 && npm run test:layer-3 && npm run test:layer-4 && npm run test:promise-event-system && npm run test:layer-5 && npm run test:layer-6 && npm run test:verify",
    "pretest:layer-1": "npm run system:start && npm run system:wait",
    "test:layer-1": "echo 'ðŸ§… Layer 1: Foundation + Browser Bootstrap' && npm run test:foundation && npm run test:browser-bootstrap",
    "posttest:layer-1": "npm run logs:show",
    "pretest:layer-2": "npm run system:start && npm run system:wait",
    "test:layer-2": "echo 'ðŸ§… Layer 2: Daemon Processes + Browser Connection' && npm run test:daemons && npm run test:browser-connection",
    "posttest:layer-2": "npm run logs:show",
    "pretest:layer-3": "npm run system:start && npm run system:wait",
    "test:layer-3": "echo 'ðŸ§… Layer 3: Message Transport + Browser WebSocket' && npm run test:transport && npm run test:browser-transport",
    "posttest:layer-3": "npm run logs:show",
    "pretest:layer-4": "npm run system:start && npm run system:wait",
    "test:layer-4": "echo 'ðŸ§… Layer 4: System Integration + Browser Commands' && npm run test:integration && npm run test:browser-commands",
    "posttest:layer-4": "npm run logs:show",
    "pretest:promise-event-system": "npm run system:start && npm run system:wait",
    "test:promise-event-system": "echo 'ðŸ§… Promise/Event System: Type-Safe Messages + Promise Chain Tests' && npx tsx tests/promise-event-system-test-suite.ts",
    "posttest:promise-event-system": "npm run logs:show",
    "pretest:layer-5": "npm run system:start && npm run system:wait",
    "test:layer-5": "echo 'ðŸ§… Layer 5: Console Interception + Browser Logging' && npm run test:console && npm run test:browser-logging",
    "posttest:layer-5": "npm run logs:show",
    "pretest:layer-6": "npm run system:start && npm run system:wait",
    "test:layer-6": "echo 'ðŸ§… Layer 6: End-to-End + Browser Automation' && npm run test:e2e && npm run test:browser-automation",
    "posttest:layer-6": "npm run logs:show && npm run system:stop",
    "test:foundation": "npx tsx tests/layer-1-foundation/types-compilation.test.ts && npx tsx tests/layer-1-foundation/encoding-abstraction.test.ts && npx tsx tests/unit/JTAGMessageTypes.test.ts && npx tsx tests/unit/ResponseCorrelator.test.ts && npx tsx tests/unit/JTAGMessageQueue.test.ts",
    "test:browser-bootstrap": "npx tsx tests/layer-1-foundation/browser-bootstrap.test.ts",
    "test:daemons": "npx tsx tests/layer-2-daemon-processes/daemon-registration.test.ts && npx tsx tests/layer-2-daemon-processes/message-routing.test.ts",
    "test:browser-connection": "npx tsx tests/layer-2-daemon-processes/browser-connection.test.ts",
    "test:transport": "npx tsx tests/layer-3-transport/websocket-transport.test.ts && npx tsx tests/layer-3-transport/encoding-transport.test.ts",
    "test:browser-transport": "npx tsx tests/layer-3-transport/browser-websocket.test.ts",
    "test:integration": "npx tsx tests/layer-4-system-integration/daemon-transport.test.ts && npx tsx tests/layer-4-system-integration/message-flow.test.ts && npx tsx tests/layer-4-system-integration/promise-chain-cross-context.test.ts",
    "test:browser-commands": "npx tsx tests/layer-4-system-integration/browser-command-execution.test.ts",
    "test:console": "npx tsx tests/layer-5-console/server-console-interception.test.ts && npx tsx tests/layer-5-console/client-console-interception.test.ts",
    "test:browser-logging": "npx tsx tests/layer-5-console/browser-console-routing.test.ts",
    "test:e2e": "npx tsx tests/layer-6-end-to-end/full-system.test.ts",
