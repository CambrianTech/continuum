  "scripts": {
    "start": "npm run system:stop && npm run clean:all && npm run build && npm run version:bump && npm pack && node scripts/update-test-bench.js && cd examples/test-bench && npm install && npm start",
    "clean:all": "rm -rf dist/ 2>/dev/null || true; rm -rf examples/dist/ 2>/dev/null || true",
    "clean:dist": "rm -rf dist/ 2>/dev/null || true",
    "prebuild": "npx tsx scripts/generate-structure.ts",
    "build": "npm run prebuild && npm run build:ts",
    "postbuild": "node scripts/create-import-map.js && node scripts/convert-imports-to-relative.js",
