{
  "name": "renderer-core",
  "version": "1.0.0",
  "description": "renderer core for Continuum",
  "main": "RendererCore.ts",
  "type": "module",
  "continuum": {
    "type": "daemon",
    "priority": 40,
    "core": "renderer",
    "category": "Core",
    "capabilities": [
      "auto-generated"
    ],
    "dependencies": [],
    "interfaces": [],
    "permissions": [],
    "quality": {
      "status": "graduated",
      "eslint": {
        "enforce": true,
        "level": "warn"
      },
      "typescript": {
        "noAny": false,
        "strict": true
      },
      "tests": {
        "required": true,
        "coverage": 75,
        "types": ["unit", "integration"]
      },
      "compliance": {
        "required": true,
        "minimumScore": 80,
        "structure": {
          "packageJson": true,
          "readme": true,
          "mainFile": true,
          "testDirectory": true
        }
      }
    }
  },
  "keywords": [
    "renderer",
    "core",
    "continuum"
  ],
  "author": "Continuum AI Platform",
  "license": "MIT",
  "scripts": {
    "test": "npx tsx test/",
    "test:unit": "npx tsx test/unit/",
    "test:integration": "npx tsx test/integration/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}