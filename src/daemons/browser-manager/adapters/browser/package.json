{
  "name": "@continuum/browser-adapter-browser",
  "version": "1.0.0",
  "description": "Browser-specific adapters and capabilities",
  "type": "module",
  "continuum": {
    "type": "browser-adapter", 
    "category": "browser-specific",
    "capabilities": [
      "chromium-devtools",
      "url-pattern-matching",
      "browser-capabilities",
      "command-line-args"
    ],
    "browsers": [
      "chromium-based"
    ],
    "dependencies": [
      "continuum-browser-adapter-base"
    ],
    "interfaces": [
      "browser-specific-adapter"
    ],
    "permissions": [
      "read"
    ]
  },
  "exports": {
    "./ChromiumBasedAdapter": "./ChromiumBasedAdapter.js"
  }
}