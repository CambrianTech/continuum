{
  "name": "@continuum/browser-adapter-os",
  "version": "1.0.0",
  "description": "OS-specific browser automation adapters",
  "type": "module", 
  "continuum": {
    "type": "os-adapter",
    "category": "platform-automation",
    "capabilities": [
      "applescript-automation",
      "script-template-system",
      "macos-browser-control"
    ],
    "platforms": [
      "darwin"
    ],
    "dependencies": [
      "continuum-browser-adapter-base"
    ],
    "interfaces": [
      "os-browser-adapter"
    ],
    "permissions": [
      "execute",
      "read-scripts",
      "system-automation"
    ]
  },
  "exports": {
    "./MacOSBrowserAdapter": "./MacOSBrowserAdapter.js"
  }
}