{
  "name": "browser-manager-daemon",
  "version": "1.0.0",
  "description": "Smart browser orchestration and connection management",
  "main": "BrowserManagerDaemon.ts",
  "type": "module",
  "keywords": ["browser", "daemon", "connection", "jtag"],
  "continuum": {
    "daemon": "browser-manager",
    "category": "Core",
    "capabilities": [
      "browser-orchestration",
      "tab-management",
      "devtools-integration",
      "session-coordination"
    ],
    "startupOrder": 100,
    "healthCheck": {
      "enabled": true,
      "intervalMs": 30000,
      "timeoutMs": 5000
    },
    "dependencies": [
      "session-manager"
    ],
    "interfaces": ["daemon-protocol", "browser-management"],
    "permissions": ["browser-control", "process-management", "session-management"]
  }
}