{
  "name": "active-projects-widget",
  "version": "1.0.0", 
  "description": "ActiveProjects Widget Module",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./component": "./ActiveProjects.js"
  },
  "files": [
    "ActiveProjects.js",
    "index.js"
  ],
  "keywords": ["widget", "projects", "ui", "continuum"],
