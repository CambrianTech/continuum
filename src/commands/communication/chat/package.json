{
  "name": "@continuum/chat-command",
  "version": "1.0.0",
  "description": "Multi-agent chat orchestration command for continuum",
  "main": "index.server.js",
  "type": "module",
  "exports": {
    ".": "./index.server.js",
    "./server": "./index.server.js"
  },
  "keywords": ["continuum", "chat", "multi-agent", "command"],
  "author": "Continuum Team",
  "license": "MIT",
  "continuum": {
    "command": "chat",
    "category": "Communication",
    "capabilities": ["session-management", "websocket-management", "message-routing"],
    "dependencies": ["base-command"],
    "interfaces": ["command-bus", "websocket-protocol"],
    "permissions": ["network-access", "websocket-server"]
  }
}