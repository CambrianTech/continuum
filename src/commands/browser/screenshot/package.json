{
  "name": "screenshot-core",
  "version": "1.0.0",
  "description": "screenshot core for Continuum",
  "main": "ScreenshotCommand.ts",
  "type": "module",
  "continuum": {
    "type": "command",
    "core": "screenshot",
    "category": "Browser",
    "capabilities": [
      "browser-screenshot",
      "visual-capture",
      "session-file-management"
    ],
    "actions": ["screenshot", "capture", "snap"],
    "exports": {
      "ScreenshotCommand": "./ScreenshotCommand.ts",
      "ScreenshotHandler": "./ScreenshotHandler.ts"
    },
    "dependencies": [],
    "interfaces": [],
    "permissions": []
  },
  "keywords": [
    "screenshot",
    "core",
    "continuum"
  ],
  "author": "Continuum AI Platform",
  "license": "MIT",
  "scripts": {
    "test": "npx tsx test/",
    "test:unit": "npx tsx test/unit/",
    "test:integration": "npx tsx test/integration/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}