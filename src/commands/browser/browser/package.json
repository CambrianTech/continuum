{
  "name": "@continuum/browser-command",
  "version": "1.0.0", 
  "description": "Browser WebSocket coordination command for continuum",
  "main": "index.server.js",
  "type": "module",
  "exports": {
    ".": "./index.server.js",
    "./server": "./index.server.js"
  },
  "keywords": ["continuum", "browser", "websocket", "command"],
  "author": "Continuum Team",
  "license": "MIT",
  "continuum": {
    "command": "browser",
    "category": "Browser",
    "capabilities": ["browser-control", "websocket-management"],
    "dependencies": ["base-command"],
    "interfaces": ["command-bus", "browser-management", "websocket-protocol"],
    "permissions": ["browser-control", "websocket-server"]
  }
}