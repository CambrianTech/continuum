{
  "name": "saveFile",
  "version": "1.0.0",
  "description": "Continuum SaveFile Command Module",
  "type": "module",
  "main": "index.server.js",
  "exports": {
    ".": {
      "node": "./index.server.js",
      "default": "./index.server.js"
    }
  },
  "continuum": {
    "command": "savefile",
    "category": "File",
    "capabilities": ["file-writing", "artifact-management"],
    "dependencies": ["base-command"],
    "interfaces": ["command-bus", "file-system"],
    "permissions": ["write", "create-files"]
  }
}