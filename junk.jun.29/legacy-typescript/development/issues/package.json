{
  "name": "issues",
  "version": "1.0.0",
  "description": "Continuum Issues Command Module - AI-driven GitHub issue management",
  "type": "module",
  "main": "index.server.js",
  "files": [
    "IssuesCommand.cjs",
    "README.md",
    "config/",
    "templates/"
  ],
  "keywords": ["issues", "github", "tickets", "ai-collaboration", "continuum", "command"],
  "scripts": {
    "test": "node --test test/*.test.js"
  },
  "continuum": {
    "commandName": "issues",
    "serverFiles": ["IssuesCommand.cjs"],
    "capabilities": ["github-api", "dashboard", "ai-collaboration", "file-sync"],
    "hasTests": true,
    "testDir": "./test",
    "configDir": "./config"
  }
}